#!/usr/bin/env bash
#SBATCH --job-name=shuffle_pixels
#SBATCH --output output_shuffle.out
#SBATCH --ntasks=1
#SBATCH --gres=gpu:1

set -euxo pipefail

# allows us to use modules within a script
source ${MODULESHOME}/init/bash

module add openmind/singularity/2.2.1

singularity exec --bind /om:/om /om/user/aaronlin/py35-tf.img python conv_shuffle.py
